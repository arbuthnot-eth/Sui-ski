{
	"version": 2,
	"builds": [
		{
			"src": "api/**/*.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/lookup",
			"dest": "/api/lookup"
		},
		{
			"src": "/reverse",
			"dest": "/api/reverse"
		},
		{
			"src": "/health",
			"dest": "/api/health"
		}
	],
	"functions": {
		"api/**/*.ts": {
			"memory": 256,
			"maxDuration": 10
		}
	}
}
